{
  "name": "grpc-nodejs-production",
  "version": "1.0.0",
  "description": "Production-level gRPC server implementation in Node.js with all RPC types",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "start:dev": "nodemon src/server/index.js",
    "client:unary": "node src/clients/unaryClient.js",
    "client:server-stream": "node src/clients/serverStreamClient.js",
    "client:client-stream": "node src/clients/clientStreamClient.js",
    "client:bidirectional": "node src/clients/bidirectionalClient.js",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "grpc",
    "nodejs",
    "microservices",
    "protobuf",
    "streaming"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
